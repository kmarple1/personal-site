  "packageManager": "yarn@4.4.0",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "jest-preview": "jest-preview",
    "lint": "yarn lint:code && yarn lint:styles",
    "lint:code": "next lint --max-warnings 0",
    "lint:styles": "stylelint '{**/*,*}.css' --ignore-path .gitignore",
    "prepare": "husky",
    "tailwind:temp": "yarn tailwindcss -i ./app/globals.css -o ./test-utils/temp.css",
    "test": "yarn tailwind:temp && jest",
    "test:dev": "yarn tailwind:temp && jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
